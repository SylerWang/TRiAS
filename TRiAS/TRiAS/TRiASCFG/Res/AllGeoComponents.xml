<!DOCTYPE GeoComponentSequence SYSTEM "GeoComponentSequence.dtd">
<GeoComponentSequence Version="1.0">
	<Name>Übersicht über die möglichen Initialisierungstags</Name>
	<GeoComponent ProgID="TRiASEx.SelectIdentObjTyp.1">
		<Name>Objektauswahl nach Objektklasse und Objekttyp</Name>
		<InitInfo>
			<Name>
				<!--Im Dialog anzuzeigendes Caption -->
				Anzuzeigendes Caption
			</Name>
			<NoShow>
				<!--NoShow=1: Dialog nicht anzeigen-->
			</NoShow>
			<SelectFrom>
				<!--SelectFrom=Document: Beschreibungsdaten anzeigen-->
				<!--SelectFrom=DataSource: Nur existierende Objektklassen anzeigen-->
			</SelectFrom>
			<Select>
				<!--Zu selektierende Objektklassen im Format [OKS]:ObjTyp(en), z.B. [12345678]:8-->
			</Select>
			<HideTypes>
				<!--Nicht anzuzeigende Objekttypen (Hexzahl: Punkte=1, Linien=2, Flächen=4, Texte=8), nicht bei SelectFrom=Document-->
			</HideTypes>
		</InitInfo>
	</GeoComponent>
	<GeoComponent ProgID="TRiASEx.TextReplaceEngine.1">
		<Name>Suchen und Ersetzen in Objekteigenschaften</Name>
		<InitInfo>
