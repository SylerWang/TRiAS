	[propget, id(2), helpstring("Liefert eine Objekteigenschaft.")] HRESULT Attribute([in] INT_PTR PrjHandle, [in] INT_PTR Obj, [in] INT_PTR Attrib, [out, retval] BSTR *AttrVal);
