// ITRiASNativeObjectsOperations Interface
[ 
	object, 
	uuid(E46774CC-7D08-40B5-8384-9F89633359FC), 
	dual, 
	helpstring("ITRiASNativeObjectsOperations Interface"),
	pointer_default(unique) 
]
interface ITRiASNativeObjectsOperations : IDispatch
{
// Methods
	[id(1), helpstring("Bildet die Vereinigungsmenge der Objekte beider übergebenen Objektmengen.")] HRESULT Union([in] ITRiASEnumNativeObjects *Objs1, [in] ITRiASEnumNativeObjects *Objs2, [out, retval] ITRiASEnumNativeObjects **ObjsOut);
	[id(2), helpstring("Bildet die Schnittmenge der Objekte beider übergebenen Objektmengen.")] HRESULT Intersection([in] ITRiASEnumNativeObjects *Obj1, [in] ITRiASEnumNativeObjects *Objs2, [out, retval] ITRiASEnumNativeObjects **ObjsOut);
	[id(3), helpstring("Bildet die Differenzmenge der Objekte beider übergebenen Objektmengen.")] HRESULT Difference([in] ITRiASEnumNativeObjects *Obj1, [in] ITRiASEnumNativeObjects *Objs2, [out, retval] ITRiASEnumNativeObjects **ObjsOut);
	[id(4), helpstring("Bildet die symmetrische Differenzmenge der Objekte beider übergebenen Objektmengen.")] HRESULT SymmetricDifference([in] ITRiASEnumNativeObjects *Obj1, [in] ITRiASEnumNativeObjects *Objs2, [out, retval] ITRiASEnumNativeObjects **ObjsOut);
	[id(5), helpstring("Stellt fest, ob die Folge der Objekte der zweiten Objektmenge vollständig in der ersten Objektmenge enthalten ist.")] HRESULT Includes([in] ITRiASEnumNativeObjects *Objs1, [in] ITRiASEnumNativeObjects *Objs2, [out, retval] VARIANT_BOOL *Includes);
};

