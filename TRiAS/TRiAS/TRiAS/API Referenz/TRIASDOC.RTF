\pard\plain \s46\sb20\sl-220\keepn\pvpara\phpg\posy0\absw1560\dxfrtext240\dfrmtxtx240\dfrmtxty240 \b\f2\fs21\lang1033 Parameter\par \pard\plain \s50\sl-240\keepn \fs21\lang1033 {\i pIniName}\par 
