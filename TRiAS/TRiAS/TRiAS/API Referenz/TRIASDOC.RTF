\pard\plain \s50\sl-240\keepn \fs21\lang1033 {\i pData}\par 
\pard\plain \s54\li280\sa80\sl-240 \fs21\lang1033 Durchzureichende Daten.\par 
\pard\plain \s46\sb20\sl-220\keepn\pvpara\phpg\posy0\absw1560\dxfrtext240\dfrmtxtx240\dfrmtxty240 \b\f2\fs21\lang1033 Siehe auch\par \pard\plain \sa160\sl-240 \fs21\lang1033 DEX_ENUMMERKMALESICHT
\par 
